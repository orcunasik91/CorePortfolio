@model List<MyService>
<div class="section px-3 px-lg-4 pt-5" id="services">
    <div class="container-narrow">
        <div class="text-center mb-5">
            <h2 class="marker marker-center">Hizmetlerim</h2>
        </div>
        <div class="text-center">
            <p class="mx-auto mb-3" style="max-width:600px"> Herhangi bir web sitesi veya uygulama için uygun hizmetler sunuyorum. Gerçek zamanlı şemalar için zamanında teslimatı hızlı bir şekilde en üst düzeye çıkarabilirim.</p>
        </div>
        <div class="row py-3">
            @{
                int delay = 100;
                foreach (MyService myService in Model)
                {
                    <div class="col-md-3 text-center" data-aos="fade-up" data-aos-delay="@delay">
                        <img class="mb-2" src="@myService.ImageUrl" width="96" height="96" alt="my service" />
                        <div class="h5">@myService.Title</div>
                    </div>
                    delay += 100;
                }
            }
        </div>
    </div>
</div>